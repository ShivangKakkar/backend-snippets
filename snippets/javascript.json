{
  "Node.js Express": {
    "prefix": "nexpress",
    "description": "Node.js Express",
    "body": [
      "import express from 'express'",
      "",
      "const app = express()",
      "",
      "app.get('/', (req, res) => {",
      "  res.send('hello world')",
      "})",
      "",
      "const port = process.env.PORT || 5000",
      "",
      "app.listen(port, () => {",
      "  console.log(`listening on port ${port}`)",
      "})"
    ]
  }
}